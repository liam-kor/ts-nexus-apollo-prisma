{
  "name": "tutorial",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --transpile-only --no-notify api/app.ts",
    "build": "tsc",
    "generate": "ts-node-dev --transpile-only api/schema.ts",
    "test": "yarn run generate && jest --detectOpenHandles --forceExit",
    "start": "node dist/api/app.js",
    "lint": "eslint ."
  },
  "jest": {
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "testEnvironment": "node"
  },
  "dependencies": {
    "@nexus/schema": "^0.19.2",
    "@prisma/client": "^2.12.1",
    "apollo-server": "^2.19.0",
    "graphql": "^15.4.0",
    "nexus-plugin-prisma": "^0.25.0"
  },
  "devDependencies": {
    "@prisma/cli": "^2.12.1",
    "@types/jest": "^26.0.15",
    "@types/pg": "^7.14.7",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "get-port": "^5.1.1",
    "graphql-request": "^3.3.0",
    "jest": "^26.6.3",
    "nanoid": "^3.1.19",
    "pg": "^8.5.1",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  }
